package com.swh.project;

import lombok.Data;

@Data
public class SendDirector {
	private A a;
	
	public SendDirector(A a) {
		// TODO Auto-generated constructor stub
		this.a = a;
	}

	public boolean senderConstruct(String fileName){
		try{
		this.a.connect();
		this.a.send(fileName);
		this.a.close();
		}catch(Exception e){
			e.printStackTrace();
			return false;
		}
		return true;
	}
}
